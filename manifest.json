{
  "description": "Control Internet, Filter Websites, The optimal use of the Internet, URL Blocker",
  "manifest_version": 2,
  "name": "Internet optimized",
  "version": "0.1",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
 
  "icons": {
    "48": "icons/Not_optimum.png"
  },
  
  "browser_action": {
  "default_icon": "icons/Not_optimum.png",
  "default_title": "icons/Internet optimized!"
},

  "background": {
    "scripts": ["optimized.js"]
  }
  
}