{
  "description": "Control Internet, Filter Websites, The optimal use of the Internet",
  "manifest_version": 2,
  "name": "Internet optimized",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
	"storage"
  ],
 
  "icons": {
    "48": "Not_optimum.png"
  },
  
  "browser_action": {
  "default_icon": "Not_optimum.png",
  "default_title": "Internet optimized!"
},

  "background": {
    "scripts": ["optimized.js"]
  }
  
}